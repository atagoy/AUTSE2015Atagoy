<?xml version="1.0" encoding="utf-8"?>
<element type="socialbookmarks" group="Social">
	<name>Social Bookmarks</name>
	<author>YOOtheme</author>
	<creationDate>March 2009</creationDate>
	<copyright>Copyright (C) 2007 - 2009 YOOtheme GmbH</copyright>
	<authorEmail>info@yootheme.com</authorEmail>
	<authorUrl>http://www.yootheme.com</authorUrl>
	<version>1.0.0</version>
	<description>Social Bookmarks</description>
	<params>
		<param name="name" type="configeditname" default="" label="Name" description="Unique name" />
		<param name="label" type="text" default="" label="Label" description="Label used in item editor" />
		<param name="description" type="text" default="" label="Description" description="Description tooltip used in item editor" />
		<param name="display" type="list" default="3" label="Display" description="Display element only on selected view">
			<option value="1">On item view</option>
			<option value="2">On category view</option>
			<option value="3">On item/category view</option>
			<option value="0">Hide</option>
		</param>
		<param name="google" type="radio" default="1" label="Google" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="technorati" type="radio" default="1" label="Technorati" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="yahoo" type="radio" default="1" label="Yahoo" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="delicious" type="radio" default="1" label="Delicious" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="stumbleupon" type="radio" default="1" label="StumbleUpon" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="digg" type="radio" default="1" label="digg" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="facebook" type="radio" default="1" label="Facebook" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="reddit" type="radio" default="1" label="Reddit" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="myspace" type="radio" default="1" label="Myspace" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="live" type="radio" default="1" label="Windows live" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="twitter" type="radio" default="1" label="Twitter" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="email" type="radio" default="1" label="Email" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
	</params>
</element>